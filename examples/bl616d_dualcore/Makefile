build_dirs += helloworld_np
build_dirs += helloworld_ap

CHIP ?= bl616d

all: $(build_dirs)

$(build_dirs):
	make -C $@

clean_all:
	@for dir in $(build_dirs); do \
		$(MAKE) -C $$dir clean; \
	done

.PHONY: $(build_dirs) clean_all

BL_SDK_BASE = ./../..

export BL_SDK_BASE

-include $(SDK_DEMO_PATH)/.config
include $(BL_SDK_BASE)/project.build